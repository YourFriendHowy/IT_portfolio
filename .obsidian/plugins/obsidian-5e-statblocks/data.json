        "spells": [
          "<STATBLOCK-WIKI-LINK>cause-fear-xge|Cause Fear<STATBLOCK-WIKI-LINK>",
          "<STATBLOCK-WIKI-LINK>Invisibility<STATBLOCK-WIKI-LINK>",
          "<STATBLOCK-WIKI-LINK>misty-step|Misty Step<STATBLOCK-WIKI-LINK>"
        ],
        "traits": [
          {
            "name": "Undead Fortitude",
            "desc": "If damage reduces Mortimer to 0 hit points, he must make a Constitution saving throw with a DC of 5 + the damage taken, unless the damage is radiant or from a critical hit. On a success, Mortimer drops to 1 hit point instead."
          },
          {
            "name": "Regeneration",
            "desc": "Mortimer gains 15 hit points at the start of its turn. If it takes cold, fire, or necrotic damage, this trait doesn't function at the start of the creature's next turn. The thorny dies only if it starts its turn with 0 hit points and doesn't regenerate."
          },
          {
            "name": "Hive mind",
            "desc": "Immune to the&nbsp;<STATBLOCK-MARKDOWN-LINK>app://obsidian.md/Z_Mechanics/CLI/rules/conditions.md#charmed|charmed<STATBLOCK-MARKDOWN-LINK>&nbsp;and&nbsp;<STATBLOCK-MARKDOWN-LINK>app://obsidian.md/Z_Mechanics/CLI/rules/conditions.md#frightened|frightened<STATBLOCK-MARKDOWN-LINK>&nbsp;conditions while within 30 feet of at least one other."
          },
          {
            "name": "Rapport Spores",
            "desc": "A 30-foot radius of spores extends from the creature. These spores can go around corners and affect only creatures with an Intelligence of 2 or higher that aren't undead, constructs, or elementals. Affected creatures can communicate telepathically with one another while they are within 30 feet of each other. The effect lasts for 1 hour."
          }
        ],
        "actions": [
          {
            "name": "Multiattack",
            "desc": "Mortimer makes two Rotting Touch attacks."
          },
          {
            "name": "Rotting Touch",
            "desc": "Melee Weapon Attack: +10 to hit, reach 10 ft., one creature. Hit: 1d8+5 (14) + 1d8 (4) necrotic damage."
          },
          {
            "name": "Spores (1/Day)",
            "desc": "A 15-foot-radius cloud of toxic spores extends out from Mortimer. The spores spread around corners. Each creature in that area that isn't a Plant must succeed on a DC 12 Constitution saving throw or be&nbsp;<STATBLOCK-MARKDOWN-LINK>app://obsidian.md/Z_Mechanics/CLI/rules/conditions.md#poisoned|poisoned<STATBLOCK-MARKDOWN-LINK>. While&nbsp;<STATBLOCK-MARKDOWN-LINK>app://obsidian.md/Z_Mechanics/CLI/rules/conditions.md#poisoned|poisoned<STATBLOCK-MARKDOWN-LINK>&nbsp;in this way, a target takes 2d8 (9) poison damage at the start of each of its turns. A target can repeat the saving throw at the end of each of its turns, ending the effect on itself on a success."
          }
        ],
        "bonus_actions": [
          {
            "name": "Flurry of Blows",
            "desc": "Mortimer can spend 1 ki point to make two unarmed strikes as a bonus action after taking the Attack action on his turn."
          },
          {
            "name": "Patient Defense",
            "desc": "Mortimer can spend 1 ki point to take the Dodge action as a bonus action on his turn."
          },
          {
            "name": "Step of the Wind",
            "desc": "Mortimer can spend 1 ki point to take the Disengage or Dash action as a bonus action on his turn, and his jump distance is doubled for the turn."
          }
        ],
        "reactions": [
          {
            "name": "Deflect Missiles",
            "desc": "Mortimer can use his reaction to deflect or catch the missile when he is hit by a ranged weapon attack. When he does so, the damage he takes from the attack is reduced by 14 (1d8 + 5). If he reduces the damage to 0, he can spend 1 ki point to make a ranged attack (as with a monk weapon) with a range of 20/60."
          }
        ],
        "source": "Homebrew"
      }
